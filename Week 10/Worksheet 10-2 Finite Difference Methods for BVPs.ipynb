      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {"id": "Task5"},
      "source": [
        "## Task 5: Mesh Refinement Study (10 minutes)\n",
        "\n",
        "Let's investigate how solution accuracy improves with finer discretizations."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {"id": "refinement_function"},
      "outputs": [],
      "source": [
        "def solve_beam_fd(n_nodes):\n",
        "    \"\"\"\n",
        "    Solve beam deflection BVP using finite differences.\n",
        "    \n",
        "    Args:\n",
        "        n_nodes: Total number of grid points\n",
        "    \n",
        "    Returns:\n",
        "        x_nodes: Grid point locations\n",
        "        y_numerical: Finite difference solution\n",
        "        y_analytical: Analytical solution at grid points\n",
        "        max_error: Maximum error in mm\n",
        "    \"\"\"\n",
        "    # Grid setup\n",
        "    h = L / (n_nodes - 1)\n",
        "    x = np.linspace(0, L, n_nodes)\n",
        "    n_interior = n_nodes - 2\n",
        "    \n",
        "    # Build tridiagonal coefficient matrix\n",
        "    A = np.diag(-2*np.ones(n_interior))\n",
        "    A += np.diag(np.ones(n_interior-1), 1)   # Upper diagonal\n",
        "    A += np.diag(np.ones(n_interior-1), -1)  # Lower diagonal\n",
        "    \n",
        "    # Build RHS vector\n",
        "    rhs_val = k * h**2\n",
        "    b = np.full(n_interior, rhs_val)\n",
        "    b[-1] -= delta  # Apply settlement BC\n",
        "    \n",
        "    # Solve system\n",
        "    y_int = np.linalg.solve(A, b)\n",
        "    \n",
        "    # Construct complete solution\n",
        "    y_num = np.zeros(n_nodes)\n",
        "    y_num[1:-1] = y_int\n",
        "    y_num[-1] = delta\n",
        "    \n",
        "    # Analytical solution at grid points\n",
        "    y_ana = analytical_solution(x)\n",
        "    \n",
        "    # Calculate error\n",
        "    max_err = np.max(np.abs(y_num - y_ana)) * 1000  # mm\n",
        "    \n",
        "    return x, y_num, y_ana, max_err\n",
        "\n",
        "# Test with our 6-node case\n",
        "x_test, y_test, y_ana_test, error_test = solve_beam_fd(6)\n",
        "print(f\"Verification: 6-node max error = {error_test:.4f} mm\")\n",
        "print(f\"Matches hand calculation: {np.max(errors):.4f} mm ‚úì\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {"id": "convergence_study"},
      "outputs": [],
      "source": [
        "# Mesh refinement study\n",
        "node_counts = [6, 11, 21, 41]\n",
        "max_errors = []\n",
        "h_values = []\n",
        "\n",
        "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(15, 6))\n",
        "\n",
        "# Left plot: Solution convergence\n",
        "ax1.plot(x_analytical, y_analytical*1000, 'k-', linewidth=4, \n",
        "         label='Analytical', alpha=0.8)\n",
        "\n",
        "colors = ['red', 'green', 'orange', 'purple']\n",
        "markers = ['o', 's', '^', 'd']\n",
        "\n",
        "for i, (n, color, marker) in enumerate(zip(node_counts, colors, markers)):\n",
        "    x_grid, y_num, y_ana, max_err = solve_beam_fd(n)\n",
        "    \n",
        "    h = L / (n - 1)\n",
        "    h_values.append(h)\n",
        "    max_errors.append(max_err)\n",
        "    \n",
        "    ax1.plot(x_grid, y_num*1000, marker + '-', color=color, \n",
        "             label=f'{n} nodes (h={h:.2f}m)', markersize=5, alpha=0.8)\n",
        "\n",
        "ax1.set_xlabel('Position x (m)', fontsize=12)\n",
        "ax1.set_ylabel('Deflection y (mm)', fontsize=12)\n",
        "ax1.set_title('Mesh Convergence Study', fontsize=14, fontweight='bold')\n",
        "ax1.grid(True, alpha=0.3)\n",
        "ax1.legend(fontsize=10)\n",
        "\n",
        "# Right plot: Error convergence\n",
        "ax2.loglog(h_values, max_errors, 'bo-', linewidth=3, markersize=8, \n",
        "           markerfacecolor='blue', markeredgecolor='darkblue')\n",
        "\n",
        "# Theoretical O(h¬≤) slope\n",
        "h_theory = np.array(h_values)\n",
        "error_theory = max_errors[0] * (h_theory / h_values[0])**2\n",
        "ax2.loglog(h_theory, error_theory, 'r--', linewidth=2, label='Theoretical O(h¬≤)')\n",
        "\n",
        "ax2.set_xlabel('Grid spacing h (m)', fontsize=12)\n",
        "ax2.set_ylabel('Maximum error (mm)', fontsize=12)\n",
        "ax2.set_title('Error Convergence Rate', fontsize=14, fontweight='bold')\n",
        "ax2.grid(True, alpha=0.3)\n",
        "ax2.legend(fontsize=10)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# Convergence table\n",
        "print(\"\\n\" + \"=\"*70)\n",
        "print(\"MESH REFINEMENT CONVERGENCE ANALYSIS\")\n",
        "print(\"=\"*70)\n",
        "print(f\"{'Nodes':<8} {'h (m)':<8} {'Max Error (mm)':<15} {'Error Ratio':<12} {'Conv. Rate':<10}\")\n",
        "print(\"-\"*70)\n",
        "\n",
        "for i, (n_nodes, h, max_err) in enumerate(zip(node_counts, h_values, max_errors)):\n",
        "    if i > 0:\n",
        "        ratio = max_errors[i-1] / max_err\n",
        "        rate = np.log(ratio) / np.log(h_values[i-1] / h)\n",
        "    else:\n",
        "        ratio = np.nan\n",
        "        rate = np.nan\n",
        "    \n",
        "    ratio_str = f\"{ratio:.2f}\" if not np.isnan(ratio) else \"-\"\n",
        "    rate_str = f\"{rate:.2f}\" if not np.isnan(rate) else \"-\"\n",
        "    print(f\"{n_nodes:<8} {h:.3f}    {max_err:.4f}          {ratio_str:<12} {rate_str:<10}\")"
      ]
